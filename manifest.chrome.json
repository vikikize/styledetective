{
  "manifest_version": 3,
  "name": "Universal DevTools Panel - Chrome",
  "version": "1.0",
  "description": "DevTools panel compatible with Chrome",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html",
  "action": {
    "default_title": "Selector Tool"
  }
}
